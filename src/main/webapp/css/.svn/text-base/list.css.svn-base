@charset "utf-8";
	body {
	font-family:"微软雅黑","Microsoft YaHei";
}
.crumbs {
	padding:0 10px;
	margin-top:6px;
	line-height:50px;
	width:1144px;
	font-size:14px;
}

.left_warp .tit {
	font-size:16px;
	line-height:40px;
	background-color:#15466E;
	height:40px;
	color:#fff;
	padding-left:15px;
	border-bottom: 2px solid #EBEBEB;
}
.left_warp .user_msg {
	height:150px;
	border-bottom:1px solid #ccc;
}
.left_warp .user_msg dl dt {
	float:left;
}
.left_warp .user_msg dl dd {
	line-height:25px;
	text-align:center;
	position:relative;
	top:30px;
}
.zhaunjifen table .tr_bj {
background-color: #EBEBEB;
height: 35px;
line-height: 35px;
font-size: 14px;
font-family: '微软雅黑';
}
.zhaunjifen table .tr_bj th {
padding-left: 15px;
}
.zhaunjifen table tr {
line-height: 34px;
height: 34px;
font-size: 14px;
font-family: "微软雅黑";
}
.left_warp .user_msg dl dt img {
	margin:18px 10px 18px 15px;
}
.left_warp .btn-submit {
	display:inline-block;
	padding:0;
	line-height:34px;
	border:1px solid #F4955B;
	color:#fff;
	font-weight:bold;
	font-size:16px;
	width:105px;
	background-color:#EF7D18;
	text-align:center;
	border-radius:3px;
	outline:none;
}
.left_warp .btn-submit:hover {
	background-color:#ff9900;
}
.left_warp .zhaunjifen {
	text-align:center;
}
.left_warp .zhaunjifen dd a img {
	margin-top:15px;
}

.jifen_main .right_warp .tit {
	height:40px;
	line-height:40px;
	font-size:16px;
	border-bottom:1px solid #ccc;
}
.right_warp .tit h3 {
	border-bottom:2px solid #ff9900;
	width:200px;
	text-indent:15px;
}
.right_warp .tit h3 span {
	font-size:14px;
	color:#D6D6D6;
	margin-left:5px;
}
.right_warp .product_top {
	padding:15px 0 5px 10px;
}
.goods_item {
	margin:0 11px 11px 0;
	font-size:12px;
	letter-spacing:normal;
	word-spacing:normal;
	vertical-align:top;
	display:inline-block;
    width:230px;
	border:1px solid #f1f1f1;
	background:#fff;
	height:315px;
	border-radius:3px;
	*float:left;
}
.goods_item .goods_img {
	position: relative;
	width: 220px;
	height: 220px;
	overflow: hidden;
	margin-left: 5px;
	margin-top: 5px;
	border: 1px solid #EBEBEB;
}
.goods_item .goods_info {
	position:relative;
	width:100%;
}
.goods_item .goods_info strong {
	padding:0 3px;
	height:30px;
	line-height:30px;
	font-weight:normal;
	overflow:hidden;
	display:block;
	font-size:14px;
	    margin-left: 5px;
}
.goods_item .goods_info .goods_price {
	height:20px;
	line-height:20px;
	color:#ff5a00;
	font-size:12px;
	margin:0 0 2px;
	overflow:hidden;
	white-space:nowrap;
	text-overflow:ellipsis;
	width:96px;
	margin-left: 5px;
}
.goods_item .goods_info .goods_price span {
	font-size:18px;
}
.goods_item .goods_info p {
	padding:0 3px;
	height:21px;
	line-height:30px;
	width:92px;
	overflow:hidden;
	white-space:nowrap;
	margin-left: 5px;
}
.goods_item .goods_info .btn_btn {
	position:absolute;
	left: 136px;
    top: 40px;
	display:inline-block;
	padding:0;
	line-height:34px;
	border:1px solid #0292e5;
	color:#fff;
	font-weight:bold;
	font-size:14px;
	width:85px;
	border-radius:3px;
	text-align:center;
	outline:none;
	background-color:#0292e5;
}
.goods_item .goods_info .btn_btn:hover {
	
}
