{
    	position:absolute;
	left:105px;
	top:40px;
	display:inline-block;
	padding:0;
	line-height:34px;
	border:1px solid #999;
	color:#fff;
	font-weight:bold;
	font-size:14px;
	width:85px;
	border-radius:3px;
	text-align:center;
	outline:none;
	background-color:#999;
    
    }
.goods_item .goods_info .btn_blue 
{
    position:absolute;
	left:105px;
	top:40px;
	display:inline-block;
	padding:0;
	line-height:34px;
	font-weight:bold;
	font-size:14px;
	width:85px;
	border-radius:3px;
	text-align:center;
	outline:none;
	background-color:#0292e5;
	color:#fff;
	border:1px solid #0178bc;
}
.suba:hover {
	color:#0178bc;
}
.goods_item:hover {
	border:1px solid #18a2f2;
}
.n-pages {
	padding:10px 60px 10px 0;
	font-size:16px;
	text-align:right;
}
.n-pages a {
	color:#7d7d7e;
	margin:0 3px;
}
.n-pages a.sel {
	color:#0178bc;
}
