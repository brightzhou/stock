/*分页样式*/
.fy{height:30px; line-height:30px; text-align:right; padding-right:20px; margin:20px 0 40px 0; *margin-top:290px;}
        .fy span,.fy a{display:inline-block; height:28px; line-height:28px; padding:0 10px; margin:0 2px; border:1px solid #cccccc; border-radius:2px; vertical-align:middle;}
        .fy a:hover{color:#2980b9; border:1px solid #2980b9;}
        .fy span i,.fy a i{display:inline-block; width:7px; height:12px; line-height:12px; font-size:0; vertical-align:middle; background:url(/img/icon/fy.gif) no-repeat;}
        .fy span.first i,.fy a.first i{background-position:0 0; margin-right:5px;}
        .fy a.first:hover i{background-position:0 -13px;}
        .fy span.last i,.fy a.last i{background-position:-8px 0; margin-left:5px;}
        .fy a.last:hover i{background-position:-8px -13px;}
        .fy .active{background-color:#2980b9; color:#fff; border:1px solid #2980b9;}
        .fy span.first, .fy span.last{color:#999;}
